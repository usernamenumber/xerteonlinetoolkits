<wizard menus="Connectors" restrict="develop">  <pageWizard remove="true" duplicate="false">    <newNodes>         <connectorPlainText><![CDATA[<connectorPlainText name="Enter Page Title" pageID="Unique ID for this page"	  navSetting="none" reportTitle="List of page titles and matching pageIDs" pageLabel="Page No" titleLabel="Title:" idLabel="pageID:" 	  notFoundMessage="could not be found in this project." notSetMessage="A destination page has not been set for this Connection"	  >Right click on a word then select Hyperlink, type or paste into the url box the following "asfunction:_level0.engine.fnTextCon,pageID" but replacing pageID with the page ID of your destination page.</connectorPlainText>]]></connectorPlainText> 	</newNodes>  </pageWizard>  <!-- TEXT PAGE =================================== -->  <connectorPlainText menu="Connectors" menuItem="Plain Text Connector" icon="icConPlainText" hint="A simple text page in which links can be made to other pages in the project." thumb="thumbs/connectorPlainText.jpg" type="text" label="Page Text1" height="250" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <pageID label="Page ID" type="TextInput" />    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" />	    <reportTitle label="Report Title" type="TextInput" wysiwyg="true" divider="true" language="true"/>    <pageLabel label="Page Label" type="TextInput" wysiwyg="true" language="true"/>    <titleLabel label="Title Label" type="TextInput" wysiwyg="true" language="true"/>    <idLabel label="PageID Label" type="TextInput" wysiwyg="true" language="true"/>    <notFoundMessage label="Not Found Message" type="TextInput" wysiwyg="true" language="true"/>    <notSetMessage label="Not Set Message" type="TextInput" wysiwyg="true" language="true"/>    <listToClip label="Output Check List" options="Don't send Page Title and IDs data to clipboard,Send Text Title and IDs List to Clipboard (Developer mode only),Send HTML Title and IDs Table to Clipboard (Developer mode only)" type="ComboBox" data="no,text,html" defaultValue="html" width="380" optional="true"/>    <textWidth type="comboBox" label="Text Width" options="Small,Medium,Large,Full" data="300,450,600,760" defaultValue="300" width="100" optional="true"/>	    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>    <info><![CDATA[<b>For full details on how to use the Plain Text Connector Page. <u><a href="http://xerte.org.uk/docs/xot/help/PlainTextConnectorPageGuide.pdf" target="_blank">Click here</a></u></b>]]></info>  </connectorPlainText></wizard>