<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>          <videoSynch><![CDATA[<videoSynch name="Enter Page Title" url="Select a Movie" text="Enter text for the page here" pause="True" erase="True"/>]]></videoSynch>    </newNodes>  	    </pageWizard>  <!-- SYNCHED VIDEO=============================================== -->  <videoSynch menu="Media" menuItem="Synched Video" hint="Synchronise passages of text to a movie." icon="icPageWhiteOffice"  thumb="thumbs/videoSynch.jpg"  remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <url label="Movie" type="Media" />    <text label="Page Text" type="TextArea"  height="200"/>    <pause label="Pause on Synchpoints" options="True,False" type="ComboBox" data="True,False" defaultValue="True"  width="100"/>    <erase label="Erase" options="True,False" type="ComboBox" data="True,False" defaultValue="True"  width="100"/>		<movieSize label="Video Size" type="textInput" defaultValue="320,240" optional="true"/>	<timedText label="Timed Text File" type="Media" optional="true"/>    <narrationNavigate label="Navigate on narration" type="CheckBox" defaultValue="false" optional="true"/>    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" optional="true"/>    <pageID label="Page ID" defaultValue="Unique ID for this page" type="TextInput" optional="true" />    <newNodes>      <nestedPage><![CDATA[<nestedPage name="name" text="Content for the pane" align="Top" synch="0"/>]]></nestedPage>    </newNodes>  </videoSynch>    <nestedPage menuItem="Nested Page" icon="icBullet" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea" height="250" />    <align label="Align Text" options="Top,Bottom" type="ComboBox" data="Top,Bottom" defaultValue="Top" width="100"/>	<linktarget label="Link from image" options="None,Original image,Specified URL" type="ComboBox"  data="None,Original image,Specified URL" defaultValue="None" width="200"/>    <link label="Specified URL" type="textInput"/>	      <url label="Image" type="media"  optional="true"/>    <tip label="Image Description" type="textInput"  optional="true" defaultValue="Enter a Description for Accessibility"/>	<synch label="Synch Point" type="NumericStepper" min="0" max="9999" step="1" defaultValue="9999" width="100" optional="true"/>    <initObject label="Init Object" type="TextInput"  optional="true"/>    <controls label="Interactivity" options="Control Bar,Native SWF" type="ComboBox" data="Control Bar,Native SWF" defaultValue="Control Bar" width="100" optional="true"/>  </nestedPage>  </wizard>