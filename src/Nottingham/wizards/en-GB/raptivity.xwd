<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>      <raptivity><![CDATA[<raptivity name="Enter Page Title" url="Select a Raptivity SWF File"/>]]></raptivity>    </newNodes>  	    </pageWizard>    <!-- RAPTIVITY============================================= -->  <raptivity menu="Media" menuItem="Raptivity SWF" hint="A page for incorporating Raptivity swfs." icon="icRaptivity"  thumb="thumbs/raptivity.jpg"  remove="true" deprecated="This page is no longer supported">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <url label="Raptivity SWF File" type="media" />    <narrationNavigate label="Navigate on narration" type="CheckBox" defaultValue="false" optional="true"/>    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" optional="true"/>    <pageID label="Page ID" defaultValue="Unique ID for this page" type="TextInput" optional="true" />  </raptivity></wizard>