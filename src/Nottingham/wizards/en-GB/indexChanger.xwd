<wizard menus="Misc" restrict="develop">  <pageWizard remove="true" duplicate="false">    <newNodes>        <indexChanger><![CDATA[<indexChanger name="Enter Page Title" instruction="Enter Instruction" prompt="Enter a Prompt" align="Left" panelWidth="Medium"	  confirmBtnTxt="Confirm" confirmBtnTip="Confirm your selection" confirmBtnWidth="100" />]]></indexChanger>	</newNodes>  </pageWizard>    <!-- INDEX CHANGER PAGE =========================================== -->  <indexChanger menu="Misc" menuItem="Index Changer" hint="Poses a question with several options, selection of which changes the project index. Use only for Xerte Desktop Page Projects" icon="icIndexChanger" thumb="thumbs/indexChanger.jpg" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <instruction label="Instruction" type="TextArea" height="100"/>	    <prompt label="Prompt" type="TextArea" height="100"/>	    <align label="Align Text" options="Left,Right" type="ComboBox" data="Left,Right" width="100"/>    <panelWidth label="Panel Width" options="Small,Medium,Large,Full" type="ComboBox" data="Small,Medium,Large,Full" width="100"/>    <confirmBtnTxt label="Confirm Button Label" type="TextInput" wysiwyg="true" divider="true" language="true"/>    <confirmBtnTip label="Confirm Button Tip" type="textInput"  language="true"/>	    <confirmBtnWidth label="Confirm Button Width" type="NumericStepper" min="100" max="250" step="10" width="100" defaultValue="100"  language="true"/>	    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" optional="true"/>    <pageID label="Page ID" defaultValue="Enter a unique ID so that Connector Pages can find this page" type="TextInput" optional="true" />	    <newNodes>      <icOption><![CDATA[<icOption name="name" text="here is an option" feedback="enter confirmation text"/>]]></icOption>    </newNodes>	  </indexChanger>      <icOption menuItem="Option" icon="icBullet" remove="true">    <name label="Option Title" type="TextInput" wysiwyg="true"/>    <text label="Option" type="TextArea" height="100"/>    <feedback label="Confirmation text" type="TextArea" height="100"/>	  </icOption></wizard>